#include "timevariablevector.h"

#ifdef GADGET_INLINE
inline
#endif
TimeVariable& TimeVariableIndexVector::operator [] (int pos) {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}

#ifdef GADGET_INLINE
inline
#endif
const TimeVariable& TimeVariableIndexVector::operator [] (int pos) const {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}
