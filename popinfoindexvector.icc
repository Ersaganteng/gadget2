#include "popinfoindexvector.h"

#ifdef GADGET_INLINE
inline
#endif
PopInfo& PopInfoIndexVector::operator [] (int pos) {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}

#ifdef GADGET_INLINE
inline
#endif
const PopInfo& PopInfoIndexVector::operator [] (int pos) const {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}
