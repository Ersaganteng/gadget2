#include "doubleindexvector.h"
#include "assert.h"

#ifdef GADGET_INLINE
inline
#endif
double& DoubleIndexVector::operator [] (int pos) {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}

#ifdef GADGET_INLINE
inline
#endif
const double& DoubleIndexVector::operator [] (int pos) const {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}
