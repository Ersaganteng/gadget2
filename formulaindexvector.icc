#include "formulaindexvector.h"

#ifdef GADGET_INLINE
inline
#endif
Formula& Formulaindexvector::operator [] (int pos) {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}

#ifdef GADGET_INLINE
inline
#endif
const Formula& Formulaindexvector::operator [] (int pos) const {
  assert(minpos <= pos && pos < minpos + size);
  return v[pos - minpos];
}
