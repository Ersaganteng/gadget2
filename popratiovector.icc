#include "popratiovector.h"

#ifdef INLINE_VECTORS
inline
#endif
popratio& popratiovector::operator [] (int pos) {
  assert(0 <= pos && pos < size);
  return(v[pos]);
}

#ifdef INLINE_VECTORS
inline
#endif
const popratio& popratiovector::operator [] (int pos) const {
  assert(0 <= pos && pos < size);
  return(v[pos]);
}
